pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- ludum dare 39: running out of power

function abs_box(s)
 local box = {}

 box.x1 = s.box.x1 + s.x
 box.y1 = s.box.y1 + s.y
 box.x2 = s.box.x2 + s.x
 box.y2 = s.box.y2 + s.y

 return box
end

function coll(a,b)
 local box_a = abs_box(a)
 local box_b = abs_box(b)

 if box_a.x1 > box_b.x2 or
    box_a.y1 > box_b.y2 or
    box_b.x1 > box_a.x2 or
    box_b.y1 > box_a.y2 then
    return false
 end

 return true

end

function create_aliens()
 
 for i = 1,10 do
  local a = {}
  a.x = i*11 - 8
  a.y = 10
  a.box = {x1=0, y1=0, x2=7, y2=7}
  a.s = 33
  add(aliens,a)
 end

 for i = 1,10 do
  local a = {}
  a.x = i*11 - 8
  a.y = 22
  a.s = 33
  a.box = {x1=0, y1=0, x2=7, y2=7}
  add(aliens,a)
 end

 for i = 1,10 do
  local a = {}
  a.x = i*11 - 8
  a.y = 34
  a.s = 33
  a.box = {x1=0, y1=0, x2=7, y2=7}
  add(aliens,a)
 end

 for i = 1,10 do
  local a = {}
  a.x = i*11 - 8
  a.y = 46
  a.s = 33
  a.box = {x1=0, y1=0, x2=7, y2=7}
  add(aliens,a)
 end
end

function update_aliens()

 if (t%4 < 10) then -- code
  aliens_steps += 1

  if (aliens_steps >= 20) then
   aliens_dir *= -1
   aliens_steps = 0

   for a in all(aliens) do
    a.y += 1
   end
   
  end

  for a in all(aliens) do
   a.x += 0.5 * aliens_dir
  end
 end

end

function draw_aliens()
print('steps ' .. aliens_steps,0,0,11)
print('aliens ' .. #aliens,0,10,11)
print('score ' .. ship.score,0,16,11)
 for a in all(aliens) do
  spr(a.s, a.x, a.y)
 end
end

function handle_game_input()
 if btn(0) then ship.x -= 2 end 
 if btn(1) then ship.x += 2 end
 -- if btn(2) then ship.y -= 1.5 end 
 -- if btn(3) then ship.y += 1.5 end

 if btnp(4) then fire() end
end

function handle_collisions()
 for b in all(bullets) do
  for a in all(aliens) do
   if coll(a,b) then
    ship.score += 1
    del(aliens,a)
    del(bullets,b)
   end
  end
 end
end

function update_ship()

 -- dont collide borders
 if (ship.x <= 1) then
  ship.x = 1
 end

 if (ship.x >= 119) then
  ship.x = 119
 end

 if (ship.y <= 0) then
  ship.y = 0
 end

 if (ship.y >= 120) then
  ship.y = 120
 end

end

function fire()
 local b = {}
 b.x = ship.x
 b.y = ship.y - 2
 b.s = 17 
 b.box = {x1=3, y1=0, x2=4, y2=7}
 add(bullets,b)
end

function update_fire()
 for b in all(bullets) do
  b.y -= 4

  if (b.y < 0) then
   del(bullets,b)
  end 
 end
end

function draw_fire()
 for b in all(bullets) do
  spr(b.s, b.x, b.y)
 end
end

function init_game()
 t = 0
 wave = 0

 ship = {}
 ship.x = 64
 ship.y = 120
 ship.s = 1
 ship.score = 0
 ship.box = {x1=0, y1=0, x2=7, y2=7}

 bullets = {}
 aliens = {}

 create_aliens()
 aliens_steps = 0
 aliens_dir = 1
end

function update_game()
 handle_game_input()
 update_ship()
 update_fire()
 update_aliens()
 handle_collisions()
end

function draw_game()
 rect(0, 0,127 ,127, 12)
 spr(ship.s, ship.x ,ship.y)
 draw_fire()
 draw_aliens()
end

function _init()
 init_game()
end

function _update()
 update_game()
end

function _draw()
 cls()
 draw_game()
end
